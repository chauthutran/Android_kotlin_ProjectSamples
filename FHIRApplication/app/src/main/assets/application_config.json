{
  "endPoint": "Patient with Medical Care",
  "appTitle" : "Simple FHIR Demo Application",
  "sync": {
    "resourcesToSync":  [
      "ActivityDefinition",
      "PlanDefinition",
      "Patient",
      "CarePlan",
      "Task",
      "Encounter",
      "Observation",
      "Questionnaire",
      "QuestionnaireResponse"
    ]
  },

  "carePlan": {
    "planDefinitionId": "1",
    "activityDefinitionId": "2",
    "taskQuestionnaireId": "10"
  },


  "patientRegistrationQuestionnaire": "Questionnaire_PatientRegistration",

  "formatDatePattern": "yyyy-MM-dd",
  "listItem": {
    "icon": "(item.gender=='female') ? 'patient_female' : 'patient_male';",
    "data": [
      {
        "value": "item.name",
        "style": "displaySmall"
      },
      {
        "value": "item.id",
        "style": "displaySmall"
      },
      {
        "value": "item.city + ', ' + item.country",
        "style": "displaySmall"
      }
    ]
  }

}