{
  "endPoint": "Patient with Medical Care",
  "sync": {
    "resourcesToSync":  [
      "Patient",
      "CarePlan",
      "Task",
      "Encounter",
      "Observation",
      "Questionnaire",
      "QuestionnaireResponse"
    ]
  },

  "patientRegistrationQuestionnaire": "Questionnaire_PatientRegistration",

  "formatDatePattern": "yyyy-MM-dd",
  "listItem": {
    "icon": "(item.gender=='female') ? 'patient_female' : 'patient_male';",
    "data": [
      {
        "value": "item.name",
        "style": "displaySmall"
      },
      {
        "value": "item.id",
        "style": "displaySmall"
      },
      {
        "value": "item.city + ', ' + item.country",
        "style": "displaySmall"
      }
    ]
  }

}