{
  "resourceType": "StructureMap",
  "id": "StructPatientRegistration",
  "text": {
    "status": "additional",
    "div": "<div xmlns=\"http://www.w3.org/1999/xhtml\"><pre>map &quot;https://com.psi.fhir/fhir/StructureMap/PatientRegistration&quot; = 'Patient Registration'\n\nuses &quot;http://hl7.org/fhir/StructureDefinition/QuestionnaireReponse&quot; as source\nuses &quot;http://hl7.org/fhir/StructureDefinition/Patient&quot; as target\n\ngroup QuestionnaireResponseToPatientMapping(source src : QuestionnaireResponse, target bundle: Bundle) {\n    src -&gt; bundle.id = uuid() &quot;r_b_id&quot;;\n    src -&gt; bundle.type = 'collection' &quot;r_bun_type&quot;;\n    src -&gt; bundle.entry as entry, entry.resource = create('Patient') as patient then\n        TransformPatient(src, patient) &quot;r_bun_entries&quot;;\n}\n\ngroup TransformPatient(source src : QuestionnaireResponse, target patient: Patient) {\n    src -&gt; patient.id = uuid() &quot;r_p_id_1&quot;;\n    src.item as item_bio where(linkId = 'PR') then { // Patient Info\n          item_bio.item as item_name where(linkId = 'PR-name') -&gt; patient.name = create('HumanName') as patientName then {\n             src -&gt; patientName.family = evaluate(item_name, $this.item.where(linkId = 'PR-name-family').answer.value) &quot;r_p_fname&quot;;\n             src -&gt; patientName.given = evaluate(item_name, $this.item.where(linkId = 'PR-name-given').answer.value) &quot;r_p_gname&quot;;\n          } &quot;r_p_name&quot;;\n\n          item_bio.item as item_dob where(linkId = 'PR-birth-date' and answer.count() &gt; 0) -&gt; patient.birthDate = evaluate(item_dob, $this.answer.value) &quot;r_p_dob&quot;;\n\n          src -&gt; patient.gender = evaluate(item_bio, $this.item.where(linkId = 'PR-gender').answer.value.code) &quot;r_p_gender&quot;;\n\n          item_bio.item as item_tele where (linkId = 'PR-telecom') -&gt; patient.telecom = create('ContactPoint') as patientContact then {\n             src -&gt; patientContact.value = evaluate(item_tele, $this.item.where(linkId = 'PR-telecom-value').answer.value) &quot;r_p_tel_num&quot;;\n             src -&gt; patientContact.system = &quot;phone&quot; &quot;r_p_tel_sys&quot;;\n          } &quot;r_p_tel&quot;;\n\n          item_bio.item as item_addr where (linkId = 'PR-address') -&gt; patient.address = create('Address') as patientAddress then {\n             src -&gt; patientAddress.city = evaluate(item_addr, $this.item.where(linkId = 'PR-address-city').answer.value) &quot;r_p_add_cty&quot;;\n             src -&gt; patientAddress.country = evaluate(item_addr, $this.item.where(linkId = 'PR-address-country').answer.value) &quot;r_p_add_cntry&quot;;\n             src -&gt; patientAddress.use = &quot;home&quot; &quot;r_p_add_use&quot;;\n             src -&gt; patientAddress.type = &quot;physical&quot; &quot;r_p_add_typ&quot;;\n          } &quot;r_p_addr&quot;;\n\n\n          src -&gt; patient.active = evaluate(item_bio, $this.item.where(linkId = 'PR-active').answer.value) &quot;r_p_act&quot;;\n\n         /* item_bio.item as item_iden where(linkId = 'PR-identifier-info') -&gt; patient.identifier = create('Identifier') as patientIdent then {\n            src -&gt; patientIdent.value = evaluate(item_iden, $this.item.where(linkId = 'PR-identifier-nid').answer.value) &quot;r_p_iden_val&quot;;\n          } &quot;r_p_iden&quot;;\n\n         item_bio.item as item_age where(linkId = 'PR-age' and answer.count() &gt; 0) -&gt; patient.birthDate = evaluate(item_age, today() - ($this.answer.value.toString() + &quot; 'years'&quot;).toQuantity()) &quot;r_p_age&quot;;\n\n          item_bio.item as item_marital where(linkId = 'PR-marital-status') -&gt; patient.maritalStatus = create('CodeableConcept') as mcc then {\n             src -&gt; mcc.coding = evaluate(item_marital, $this.answer.value) &quot;r_p_martial_cod&quot;;\n          } &quot;r_p_marital&quot;;\n\n          item_bio.item as item_tele where (linkId = 'PR-telecom') -&gt; patient.telecom = create('ContactPoint') as patientContact then {\n             src -&gt; patientContact.value = evaluate(item_tele, $this.item.where(linkId = 'PR-telecom-value').answer.value) &quot;r_p_tel_num&quot;;\n             src -&gt; patientContact.system = &quot;phone&quot; &quot;r_p_tel_sys&quot;;\n          } &quot;r_p_tel&quot;;\n\n          item_bio.item as item_eth where (linkId = 'PR-ethnicity-group') -&gt; patient.extension = create('Extension') as patientEthnicity then {\n            src -&gt; patientEthnicity.url = evaluate(item_eth, $this.item.where(linkId = 'PR-ethnicity-authority').answer.value) &quot;r_p_eth_url&quot;;\n            src -&gt; patientEthnicity.value = evaluate(item_eth, $this.item.where(linkId = 'PR-ethnicity').answer.value) &quot;r_p_eth_val&quot;;\n          } &quot;r_p_eth&quot;;\n\n          item_bio.item as item_occ where (linkId = 'PR-occupation-group') -&gt; patient.extension = create('Extension') as patientOccupation then {\n           src -&gt; patientOccupation.url = evaluate(item_occ, $this.item.where(linkId = 'PR-occupation-authority').answer.value) &quot;r_p_occ_url&quot;;\n           src -&gt; patientOccupation.value = evaluate(item_occ, $this.item.where(linkId = 'PR-occupation').answer.value) &quot;r_p_occ_val&quot;;\n          } &quot;r_p_occ&quot;;  */\n\n       };\n\n}\n\n</pre></div>"
  },
  "url": "https://com.psi.fhir/fhir/StructureMap/PatientRegistration",
  "name": "Patient Registration",
  "structure": [
    {
      "url": "http://hl7.org/fhir/StructureDefinition/QuestionnaireReponse",
      "mode": "source"
    },
    {
      "url": "http://hl7.org/fhir/StructureDefinition/Patient",
      "mode": "target"
    }
  ],
  "group": [
    {
      "name": "QuestionnaireResponseToPatientMapping",
      "typeMode": "none",
      "input": [
        {
          "name": "src",
          "type": "QuestionnaireResponse",
          "mode": "source"
        },
        {
          "name": "bundle",
          "type": "Bundle",
          "mode": "target"
        }
      ],
      "rule": [
        {
          "name": "r_b_id",
          "source": [
            {
              "context": "src"
            }
          ],
          "target": [
            {
              "context": "bundle",
              "contextType": "variable",
              "element": "id",
              "transform": "uuid"
            }
          ]
        },
        {
          "name": "r_bun_type",
          "source": [
            {
              "context": "src"
            }
          ],
          "target": [
            {
              "context": "bundle",
              "contextType": "variable",
              "element": "type",
              "transform": "copy",
              "parameter": [
                {
                  "valueString": "collection"
                }
              ]
            }
          ]
        },
        {
          "name": "r_bun_entries",
          "source": [
            {
              "context": "src"
            }
          ],
          "target": [
            {
              "context": "bundle",
              "contextType": "variable",
              "element": "entry",
              "variable": "entry"
            },
            {
              "context": "entry",
              "contextType": "variable",
              "element": "resource",
              "variable": "patient",
              "transform": "create",
              "parameter": [
                {
                  "valueString": "Patient"
                }
              ]
            }
          ],
          "dependent": [
            {
              "name": "TransformPatient",
              "variable": [
                "src",
                "patient"
              ]
            }
          ]
        }
      ]
    },
    {
      "name": "TransformPatient",
      "typeMode": "none",
      "input": [
        {
          "name": "src",
          "type": "QuestionnaireResponse",
          "mode": "source"
        },
        {
          "name": "patient",
          "type": "Patient",
          "mode": "target"
        }
      ],
      "rule": [
        {
          "name": "r_p_id_1",
          "source": [
            {
              "context": "src"
            }
          ],
          "target": [
            {
              "context": "patient",
              "contextType": "variable",
              "element": "id",
              "transform": "uuid"
            }
          ]
        },
        {
          "name": "item",
          "source": [
            {
              "context": "src",
              "element": "item",
              "variable": "item_bio",
              "condition": "(linkId = 'PR')"
            }
          ],
          "rule": [
            {
              "name": "r_p_name",
              "source": [
                {
                  "context": "item_bio",
                  "element": "item",
                  "variable": "item_name",
                  "condition": "(linkId = 'PR-name')"
                }
              ],
              "target": [
                {
                  "context": "patient",
                  "contextType": "variable",
                  "element": "name",
                  "variable": "patientName",
                  "transform": "create",
                  "parameter": [
                    {
                      "valueString": "HumanName"
                    }
                  ]
                }
              ],
              "rule": [
                {
                  "name": "r_p_fname",
                  "source": [
                    {
                      "context": "src"
                    }
                  ],
                  "target": [
                    {
                      "context": "patientName",
                      "contextType": "variable",
                      "element": "family",
                      "transform": "evaluate",
                      "parameter": [
                        {
                          "valueId": "item_name"
                        },
                        {
                          "valueString": "$this.item.where(linkId = 'PR-name-family').answer.value"
                        }
                      ]
                    }
                  ]
                },
                {
                  "name": "r_p_gname",
                  "source": [
                    {
                      "context": "src"
                    }
                  ],
                  "target": [
                    {
                      "context": "patientName",
                      "contextType": "variable",
                      "element": "given",
                      "transform": "evaluate",
                      "parameter": [
                        {
                          "valueId": "item_name"
                        },
                        {
                          "valueString": "$this.item.where(linkId = 'PR-name-given').answer.value"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "name": "r_p_dob",
              "source": [
                {
                  "context": "item_bio",
                  "element": "item",
                  "variable": "item_dob",
                  "condition": "((linkId = 'PR-birth-date') and (answer.count() > 0))"
                }
              ],
              "target": [
                {
                  "context": "patient",
                  "contextType": "variable",
                  "element": "birthDate",
                  "transform": "evaluate",
                  "parameter": [
                    {
                      "valueId": "item_dob"
                    },
                    {
                      "valueString": "$this.answer.value"
                    }
                  ]
                }
              ]
            },
            {
              "name": "r_p_gender",
              "source": [
                {
                  "context": "src"
                }
              ],
              "target": [
                {
                  "context": "patient",
                  "contextType": "variable",
                  "element": "gender",
                  "transform": "evaluate",
                  "parameter": [
                    {
                      "valueId": "item_bio"
                    },
                    {
                      "valueString": "$this.item.where(linkId = 'PR-gender').answer.value.code"
                    }
                  ]
                }
              ]
            },
            {
              "name": "r_p_tel",
              "source": [
                {
                  "context": "item_bio",
                  "element": "item",
                  "variable": "item_tele",
                  "condition": "(linkId = 'PR-telecom')"
                }
              ],
              "target": [
                {
                  "context": "patient",
                  "contextType": "variable",
                  "element": "telecom",
                  "variable": "patientContact",
                  "transform": "create",
                  "parameter": [
                    {
                      "valueString": "ContactPoint"
                    }
                  ]
                }
              ],
              "rule": [
                {
                  "name": "r_p_tel_num",
                  "source": [
                    {
                      "context": "src"
                    }
                  ],
                  "target": [
                    {
                      "context": "patientContact",
                      "contextType": "variable",
                      "element": "value",
                      "transform": "evaluate",
                      "parameter": [
                        {
                          "valueId": "item_tele"
                        },
                        {
                          "valueString": "$this.item.where(linkId = 'PR-telecom-value').answer.value"
                        }
                      ]
                    }
                  ]
                },
                {
                  "name": "r_p_tel_sys",
                  "source": [
                    {
                      "context": "src"
                    }
                  ],
                  "target": [
                    {
                      "context": "patientContact",
                      "contextType": "variable",
                      "element": "system",
                      "transform": "copy",
                      "parameter": [
                        {
                          "valueString": "phone"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "name": "r_p_addr",
              "source": [
                {
                  "context": "item_bio",
                  "element": "item",
                  "variable": "item_addr",
                  "condition": "(linkId = 'PR-address')"
                }
              ],
              "target": [
                {
                  "context": "patient",
                  "contextType": "variable",
                  "element": "address",
                  "variable": "patientAddress",
                  "transform": "create",
                  "parameter": [
                    {
                      "valueString": "Address"
                    }
                  ]
                }
              ],
              "rule": [
                {
                  "name": "r_p_add_cty",
                  "source": [
                    {
                      "context": "src"
                    }
                  ],
                  "target": [
                    {
                      "context": "patientAddress",
                      "contextType": "variable",
                      "element": "city",
                      "transform": "evaluate",
                      "parameter": [
                        {
                          "valueId": "item_addr"
                        },
                        {
                          "valueString": "$this.item.where(linkId = 'PR-address-city').answer.value"
                        }
                      ]
                    }
                  ]
                },
                {
                  "name": "r_p_add_cntry",
                  "source": [
                    {
                      "context": "src"
                    }
                  ],
                  "target": [
                    {
                      "context": "patientAddress",
                      "contextType": "variable",
                      "element": "country",
                      "transform": "evaluate",
                      "parameter": [
                        {
                          "valueId": "item_addr"
                        },
                        {
                          "valueString": "$this.item.where(linkId = 'PR-address-country').answer.value"
                        }
                      ]
                    }
                  ]
                },
                {
                  "name": "r_p_add_use",
                  "source": [
                    {
                      "context": "src"
                    }
                  ],
                  "target": [
                    {
                      "context": "patientAddress",
                      "contextType": "variable",
                      "element": "use",
                      "transform": "copy",
                      "parameter": [
                        {
                          "valueString": "home"
                        }
                      ]
                    }
                  ]
                },
                {
                  "name": "r_p_add_typ",
                  "source": [
                    {
                      "context": "src"
                    }
                  ],
                  "target": [
                    {
                      "context": "patientAddress",
                      "contextType": "variable",
                      "element": "type",
                      "transform": "copy",
                      "parameter": [
                        {
                          "valueString": "physical"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "name": "r_p_act",
              "source": [
                {
                  "context": "src"
                }
              ],
              "target": [
                {
                  "context": "patient",
                  "contextType": "variable",
                  "element": "active",
                  "transform": "evaluate",
                  "parameter": [
                    {
                      "valueId": "item_bio"
                    },
                    {
                      "valueString": "$this.item.where(linkId = 'PR-active').answer.value"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
